    "# Задача 1\n",
    "##  Напишите функцию, которая на вход принимает два словаря и возвращает один объединенный словарь"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "{'One': 1, 'Two': 2, 'Three': 3, 'For': 4, 'Five': 5, 'Six': 6, 'Seven': 7}"
      ]
     },
     "execution_count": 9,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "def join_dict(d1, d2:list):\n",
    "    return {**d1, **d2}\n",
    "\n",
    "dict1 = {'One': 1, 'Two': 2, 'Three': 3}\n",
    "dict2 = {'For': 4, 'Five': 5, 'Six': 6, 'Seven': 7}\n",
    "join_dict (dict1, dict2)\n"
   ]
  }
